<div class="row mt-5">
    <div class="col-md-6 m-auto">
      <div class="card card-body">
        <h1 class="text-center mb-3">
          <i class="fas fa-user-plus"></i> Register
        </h1>
        <form action="/update/contact/<%-data.id%>" method="POST">
          <div class="form-group">
            <label for="name">Name</label>
            <input
              type="name"
              id="name"
              name="name"
              class="form-control"
              value="<%=data.name%>"
            />
          </div>
          <div class="form-group">
            <label for="phone">Ph#</label>
            <input
              type="text"
              id="phone"
              name="phone"
              class="form-control"
              value="<%=data.phone%>"
            />
          </div>
          <div class="form-group">
            <label for="email">Email</label>
            <input
              type="email"
              id="email"
              name="email"
              class="form-control"
              value="<%=data.email%>"
            />
          </div>
          <button type="submit" class="btn btn-primary btn-block">
            Update
          </button>
          
        </form>
        <a href="/dashboard" style="text-decoration: none; margin-top: 6px;"><button type="submit" class="btn btn-secondary btn-block">
            Cancel
        </button></a>
      </div>
    </div>
  </div>
  